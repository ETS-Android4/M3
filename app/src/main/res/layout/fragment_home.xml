<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:attv="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fhome">

    <TextView
        android:id="@+id/ProfileTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:text="@string/home"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold" />

    <com.dragankrstic.autotypetextview.AutoTypeTextView
        android:id="@+id/AutoTypeLabel"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="100dp"
        android:textSize="18sp"
        attv:typingPrecision="1"
        />
    <!--
        attv:animateTextTypeWithoutMistakes="Hello User, How are you doing today?"
        -->

    <Button
        android:id="@+id/demosound"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="200dp"
        android:text="Play sound" />

    <Button
        android:id="@+id/demosound1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="250dp"
        android:text="Stop sound" />


</RelativeLayout>